<p id="notice"><%= notice %></p>

<h1>Listing Moderators</h1>

<table>
  <thead>
    <tr>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @moderators.each do |moderator| %>
      <tr>
        <td><%= moderator.user.username %></td>
        <td><%= link_to 'Destroy', [@subclonnit, moderator], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if @can_add %>
  <%= link_to 'New Moderator', new_subclonnit_moderator_path(@subclonnit) %>
<% end %>
